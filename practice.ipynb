   "source": [
    "# n, k 입력\n",
    "n, k = map(int, input().split())\n",
    "\n",
    "count = 0\n",
    "\n",
    "while(True):\n",
    "    if n % k == 0:\n",
    "        n = int(n / k)\n",
    "    else:\n",
    "        n -= 1\n",
    "    \n",
    "    count += 1\n",
    "    \n",
    "    if n == 1:\n",
    "        break\n",
    "       \n",
    "\n",
    "print(count)\n"
   ]
